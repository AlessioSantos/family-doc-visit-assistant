{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Output",
  "type": "object",
  "required": [
    "summary",
    "draft_note",
    "missing_info"
  ],
  "properties": {
    "summary": {
      "type": "string"
    },
    "draft_note": {
      "type": "string"
    },
    "missing_info": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "risk_level": {
      "type": "string",
      "enum": [
        "LOW",
        "MED",
        "HIGH"
      ],
      "default": "LOW"
    },
    "risk_flags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}